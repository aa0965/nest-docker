{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/protocol-transformer/src",
  "monorepo": true,
  "root": "apps/protocol-transformer",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/protocol-transformer/tsconfig.app.json",
    "plugins": [
      "@nestjs/swagger/plugin"
    ]
  },
  "projects": {
    "protocol-transformer": {
      "type": "application",
      "root": "apps/protocol-transformer",
      "entryFile": "main",
      "sourceRoot": "apps/protocol-transformer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/protocol-transformer/tsconfig.app.json"
      }
    },
    "epts": {
      "type": "application",
      "root": "apps/epts",
      "entryFile": "main",
      "sourceRoot": "apps/epts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/epts/tsconfig.app.json"
      }
    },
    "packet-types": {
      "type": "library",
      "root": "libs/types/packet-types",
      "entryFile": "index",
      "sourceRoot": "libs/types/packet-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/types/packet-types/tsconfig.lib.json"
      }
    },
    "devices-types": {
      "type": "library",
      "root": "libs/types/devices-types",
      "entryFile": "index",
      "sourceRoot": "libs/types/devices-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/types/devices-types/tsconfig.lib.json"
      }
    },
    "sample-setup": {
      "type": "application",
      "root": "apps/sample-setup",
      "entryFile": "main",
      "sourceRoot": "apps/sample-setup/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sample-setup/tsconfig.app.json"
      }
    }
  }
}